{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "JSON schema for the Web Accessibility rules.",
	"type": "object",
	"rules": [
		{
			"type": "Semantic",
			"identifier": [
				"<div(?:.|)+?>",
				"<span(?:.|)+?>"
			],
			"severity": 3,
			"message": "Use Semantic HTML5 or specify a WAI-ARIA role [role=\"\"]",
			"filters": [
				{
					"identifier": "role=(?:.*?[a-z].*?)\"",
					"options": {
						"contains": false,
						"negative": true,
						"replace": false
					}
				}
			]
		}
	]
}